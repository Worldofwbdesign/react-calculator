    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "cross-env": "^5.0.0",
    "express": "^4.13.4",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "lodash": "^3.10.1",
    "material-ui": "^0.18.5",
    "mocha": "^2.4.5",
    "node-env-file": "^0.1.8",
